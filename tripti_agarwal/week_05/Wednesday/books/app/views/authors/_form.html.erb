<%= form_for @author do |f| %>
  <fieldset>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </fieldset>
  <fieldset>
    <%= f.label :dob %>
    <%= f.text_field :dob%>
  </fieldset>
  <fieldset>
    <%= f.label :image %>
    <%= f.url_field :image %>
  </fieldset>
  <fieldset>
    <%= f.label :nationality %>
    <%= f.text_field :nationality %>
  </fieldset>
  <fieldset>
    <%= f.label :styles %>
    <%= f.text_field :styles %>
  </fieldset>
  <%= f.submit %>
<% end %>
